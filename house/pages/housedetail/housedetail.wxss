page {
  background: #fff;
  font-family: 'PingFang SC';
}
/* 轮播图 */
.swiper-container{
  position: relative;
}
.swiper-container .swiper{
   height: 280px;
}
.swiper-container .swiper .img{
  width: 100%;
  height: 280px;

}

.swiper-container .dots{
  position: absolute;
  right: 40rpx;
  bottom: 30rpx;
  display: flex;
  justify-content: center;
  
}
.swiper-container .dots .dot{
  color: #fff;
  margin: 0 10rpx;
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  background:black;
  border-radius:25rpx;
  text-align: center;
  font-size: 22rpx;
  opacity:0.6;  
}
.swiper-container .dots_name{
  position: absolute;
  left: 40rpx;
  bottom: 30rpx;
  display: flex;
  justify-content: center;
  
}

.swiper-container .dots_name .dots_namehouse{
  color: #fff;
  margin: 0 10rpx;
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  background:black;
  border-radius:25rpx;
  text-align: center;
  font-size: 22rpx;
  opacity:0.6;  
}

/* 房屋内容 */
.content_house{
   width: 100%;
   background-color:#fff;
}
.content_house .house_name{
  width: 88%;
  height: 160rpx;
  line-height: 160rpx;
  margin: 0 auto;
  box-sizing: border-box;
  color: #333333;
  font-size: 36rpx;
  font-weight: bold;
  border-bottom: 2rpx solid #E7EBF1;
}
.content_house .house_type{
  width: 88%;
  height: 160rpx;
  margin: 0 auto;
  box-sizing: border-box;
  display: flex;
  font-size: 26rpx;
  align-items: center;
  border-bottom: 2rpx solid #E7EBF1;
  font-weight: bold;
}
.content_house .house_type .typeone{
  flex: 1;
  height: 70rpx;
  border-right: 4rpx solid #E7EBF1;
}
.content_house .house_type .typeone .houseprice{
  color: #666666;
}
.content_house .house_type .typeone .monthprice{
  color: #FF4A5B;
  font-size: 30rpx;
  padding-top: 6rpx;
}
.content_house .house_type .typetwo{
  flex: 1;
  height: 70rpx;
  border-right: 4rpx solid #E7EBF1;
  margin-left: 40rpx
}
.content_house .house_type .typetwo .houseprice{
  color: #666666;
}
.content_house .house_type .typetwo .monthprice{
  color: #FF4A5B;
  font-size: 30rpx;
  padding-top: 6rpx;
}
.content_house .house_type .typethree{
  flex: 1;
  height: 70rpx;
  margin-left: 40rpx
}
.content_house .house_type .typethree .houseprice{
  color: #666666;
}
.content_house .house_type .typethree .monthprice{
  color: #FF4A5B;
  font-size: 28rpx;
  padding-top: 6rpx;
 
}
/* 房屋小图标 */
.content_smallimg{
  width: 88%;
  height: 100rpx;
  margin: 0 auto;
  font-size: 20rpx;
  color: #88ACE2;
  box-sizing: border-box;
  padding-top: 24rpx;

}
.content_smallimg .smalllo{
  height: 80rpx;
  width: 100%;
  overflow: hidden;
  line-height: 40rpx;

}
.content_smallimg .smalllogo{
  background-color:#E7EBF1;
  margin-left: 10rpx;
  padding: 4rpx;
}


/* 房屋信息*/
.content_housemessage{
  width: 88%;
  margin: 0 auto; 
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20rpx
}
.content_housemessage .housemessage{

  width: 50%;
  height: 70rpx;
  flex-wrap: wrap;
  font-size: 30rpx;
  line-height: 70rpx; 
}
.content_housemessage .housemessage .housename{
  color: #7A7F82;
}
.content_housemessage .housemessage .housecontent{
  color: #000000;
  padding-left: 20rpx;
}
/*小区名称*/
.content_village{
  height: 70rpx;
  width: 100%;
  line-height: 70rpx;
  font-size: 30rpx;
}
.content_village .housename{
  color: #7A7F82;
}
.content_village .housecontent{
  color: #000000;
  padding-left: 20rpx;
}
/* 分割线 */
.division{
 width: 100%;
 height: 20rpx;
 background-color: #E7EBF1;
}

/*房源简介*/
.house_source_introduction{
  width: 100%;
  height: 360rpx;
  margin: 0 auto;
  box-sizing: border-box;
}
.house_source_introduction .introduction{
  height: 120rpx; 
  width: 88%;
  line-height: 120rpx;
  font-size: 38rpx;
  color: #000000;
  margin: 0 auto;
}
.house_source_introduction .introduction_img{
  width: 100%; 
  height: 320rpx;
}
.house_source_introduction .introduction_img .introduction_img_tb{
  display: flex;
  width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  padding-top: 20rpx;
  text-align: center;
}
.house_source_introduction .introduction_img .introduction_img_tba{
  flex:1;
}
.house_source_introduction .introduction_img .introduction_img_tba .introduction_miaosu{
  font-size: 20rpx;
}
.house_source_introduction .introduction_img .introduction_img_tba .introduction_common_img{
  font-size: 44rpx;
}
.act_img{
  color:#7A7F82;
}
.act_imga{
  color:#7A7F82;
  text-decoration: line-through;
}
/*房源简介文字*/
.textintroduction{
  width: 88%;
  margin: 0 auto;
  font-size: 24rpx;
  border-top: 1rpx solid #E7EBF1;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  position: relative;
}
.textintroduction .textintroduction_txt{
  display: -webkit-box;
  overflow: hidden;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.textintroduction .activity{
  font-size: 24rpx;
  display: -webkit-box;
  overflow: hidden;
  word-wrap: break-word;
  white-space: normal !important;

  -webkit-box-orient: vertical;
}
.textintroduction .textintroduction_more{
  position: absolute;
   right: 0;
  bottom: 30rpx;
  display: flex;
  background-color:#fff;
  width: 80rpx;
  height: 30rpx;
  justify-content: center;
  box-shadow: 2px 2px 2px #fff;
}

.foot_f{
  height: 200rpx;
  width: 100%;
  background-color: #E7EBF1
}

/*地图*/
.footmap{
  width: 100%;
  height: 600rpx;
  position: relative;
}
.footmap .footmapb{
  width: 90%;
  height: 130rpx;
  color: #000000;
  font-size: 38rpx;
  line-height: 130rpx;
  margin: 0 auto;
}

/**付款方式*/
.paytype{
  width: 100%;
  height: 280rpx;
}
.paytype .tpyem{
  width: 88%;
  height: 130rpx;
  color: #000000;
  font-size: 38rpx;
  line-height: 130rpx;
  margin: 0 auto;
  }
.paytype .typemoney{
  display: flex;
  width: 88%;
  height: 130rpx;
  margin: 0 auto;
}
.paytype .act{
  padding-top: 20rpx
}
.paytype .typemoney .typemoneya{
  flex: 1;
}
.paytype .typemoney .typemoneyatxt{
  font-size: 30rpx;
  color: #000000;
}
.paytype .typemoney .typemoneyatxta{
  color: #666666;
  padding-top: 18rpx;
  font-size: 24rpx;

}
.paytype .typemoney .typemoneyb{
  flex: 1;
  text-align: right;
  font-size: 30rpx;
  color: #FE4F53;
  padding-top: 16rpx;
}
/* 底部 */
.foot{
  width:100%; 
  height: 120rpx; 
  position:fixed; 
  bottom:0; 
  box-sizing: border-box;
  border: 1rpx solid #F0F0F0;
}
.foot_v{
  display: flex;
  width:100%; 
  height: 80rpx; 
  padding: 20rpx 0;
  background-color: #fff;
}
.foot_v_img{
  flex: 1;
  text-align: center;
  color: #333;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 80rpx;
}

.foot_v_img_txt{
  font-size: 22rpx;
 
}
.foot_v_phone{
  flex: 2;
}
.foot_v_phone_btn{
  width: 400rpx;
  height: 80rpx;
  background: linear-gradient(70deg,#6299FF,#2A6CE2);
  border-radius: 7rpx;
  /* margin-top: 20rpx; */
  line-height: 70rpx;
  color: #fff;
  text-align: center;
}
.foot_v_dianhua{
   margin-right: 12rpx;
   font-size: 28rpx;
}
.foot_v_dianhua_txt{
  font-size: 30rpx;
}
